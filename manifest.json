{
  "name": "Eventbrite Chrome Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Discover Great Events Around You With Eventbrite",
  "homepage_url": "https://github.com/minseokim/eventbrite-chrome-extension",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://maps.googleapis.com https://maps.gstatic.com; object-src 'self'"
}